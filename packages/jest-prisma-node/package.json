{
  "name": "@quramy/jest-prisma-node",
  "version": "1.5.0",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "engines": {
    "node": ">=14.13"
  },
  "exports": {
    ".": {
      "module": "./index.mjs",
      "types": "./lib/index.d.ts",
