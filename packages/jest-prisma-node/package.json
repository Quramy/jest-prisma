  },
  "dependencies": {
    "@quramy/jest-prisma-core": "^1.0.0"
  },
  "peerDependencies": {
    "jest": "^28.0.0 || ^29.0.0",
    "jest-environment-node": "^28.0.0 || ^29.0.0"
  },
  "scripts": {
    "bootstrap": "tsc",
    "test:ci": "echo \"test\"",
    "test": "echo \"test\"",
    "prepublish": "cp ../../README.md ./"
  },
  "keywords": [
    "jest",
    "jest-environment",
    "prisma"
  ],
  "author": "Quramy",
  "license": "MIT"
