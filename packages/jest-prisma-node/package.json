      "import": "./environment.mjs",
      "require": "./lib/environment.js",
      "default": "./lib/environment.js"
    }
  },
  "files": [
    "*.mjs",
    "lib/**/*"
  ],
  "homepage": "https://github.com/Quramy/jest-prisma",
  "repository": {
